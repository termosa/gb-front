{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "NextJS boilerplate with TypeScript and Redux",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:dev": "NODE_ENV=test jest --watchAll",
    "start": "NODE_ENV=production PORT=$PORT next start",
    "build": "rm -rf .next && next build",
    "dev": "next dev",
    "export": "npm run build && next export",
    "prettier": "prettier  --write **/*.{ts,tsx,js}",
    "lint:ts": "npm run prettier && eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:scss": "stylelint --fix 'src/**/*.scss'",
    "lint:types": "tsc -p . --noEmit",
    "start:library": "bit link && bit start",
    "create:component": "bit create generators/component -n components",
    "create:module": "bit create generators/module -n modules",
    "upgrade:bit": "yarn upgrade --scope @fragrantjewels"
  },
  "dependencies": {
    "@fragrantjewels/gravity-brands.components.category-shop-card": "^0.0.8",
    "@fragrantjewels/gravity-brands.components.footer": "^0.0.7",
    "@fragrantjewels/gravity-brands.components.hero-gallery": "^0.0.9",
    "@fragrantjewels/gravity-brands.components.inline-signup-form": "^0.0.11",
    "@fragrantjewels/gravity-brands.components.inner-circle-exclusive": "^0.0.8",
    "@fragrantjewels/gravity-brands.components.main-page-layout": "^0.0.3",
    "@fragrantjewels/gravity-brands.components.products-carousel": "^0.0.1",
    "@fragrantjewels/gravity-brands.components.promise-bar": "^0.0.10",
    "@fragrantjewels/gravity-brands.components.reviews-section": "^0.0.8",
    "@fragrantjewels/gravity-brands.components.rolling-banner": "^0.0.7",
    "@fragrantjewels/gravity-brands.components.shop-by-products-overview": "^0.0.10",
    "@fragrantjewels/gravity-brands.helpers.theme-provider": "^0.0.7",
    "@fragrantjewels/gravity-brands.modules.base-api-url": "^0.0.3",
    "@fragrantjewels/gravity-brands.modules.load-collection": "^0.0.8",
    "@fragrantjewels/gravity-brands.modules.load-product": "^0.0.7",
    "@fragrantjewels/gravity-brands.modules.resolve-page-props": "^0.0.2",
    "@fragrantjewels/gravity-brands.modules.signup": "^0.0.9",
    "classnames": "^2.3.1",
    "next": "10.0.3",
    "node-sass": "^5.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-media-hook": "^0.4.9",
    "react-slick": "^0.28.1",
    "styled-components": "^5.2.3",
    "svg-sprite-loader": "4.2.1",
    "use-defer": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@teambit/cli": "^0.0.352",
    "@teambit/generator": "^0.0.375",
    "@teambit/harmony": "^0.2.11",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "11.2.2",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "25.1.4",
    "@types/lodash": "^4.14.169",
    "@types/node": "13.9.5",
    "@types/react": "16.9.27",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.9",
    "@types/svg-sprite-loader": "3.9.1",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "acorn": "7.1.1",
    "babel-core": "6.26.3",
    "babel-jest": "25.2.4",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.2.4",
    "jest-svg-transformer": "1.0.0",
    "lint-staged": "10.1.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.2",
    "stylelint": "13.2.1",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-scss": "3.16.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "typescript": "3.8.3"
  },
  "engines": {
    "npm": "NPM IS NOT SUPPORTED, USE YARN INSTEAD: npm install -g yarn && yarn",
    "yarn": ">= 1.17.3",
    "node": ">= 12.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "keywords": [
    "nextjs",
    "boilerplate",
    "typeccript",
    "react",
    "prettier",
    "stylelint",
    "jest",
    "enzyme",
    "svg sprite",
    "husky",
    "lint staged"
  ],
  "author": "Sanjay Twisk",
  "repository": {
    "type": "git",
    "url": "git@github.com:sanjaytwisk/nextjs-ts.git"
  },
  "license": "ISC"
}
