{
  "name": "fragrantjewels-front",
  "version": "1.0.0",
  "description": "NextJS boilerplate with TypeScript and Redux",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:dev": "NODE_ENV=test jest --watchAll",
    "start": "PORT=$PORT next start",
    "build": "rm -rf .next && next build",
    "dev": "next dev",
    "export": "npm run build && next export",
    "prettier": "prettier  --write **/*.{ts,tsx,js}",
    "lint:ts": "npm run prettier && eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:scss": "stylelint --fix 'src/**/*.scss'",
    "lint:types": "tsc -p . --noEmit",
    "bit": "bit link && bit start",
    "start:library": "npm run bit",
    "proxy": "lcp --proxyUrl http://k8s-default-djangolb-f355e68fee-da67faa0143d6707.elb.us-west-1.amazonaws.com --proxyPartial \"\" --port 3033",
    "proxy:prod": "lcp --proxyUrl http://k8s-default-djangolb-766aaf61e8-14636e29744cd427.elb.us-west-2.amazonaws.com --proxyPartial \"\" --port 3033",
    "docs": "parcel -d .docs-dist --cache-dir .docs-cache docs/index.html",
    "create:component": "bit create generators/component -n components",
    "create:module": "bit create generators/module -n modules",
    "upgrade:scope": "node scripts/upgrade-fragrant-scope-versions.js"
  },
  "dependencies": {
